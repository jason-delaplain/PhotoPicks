{
  "expo": {
    "name": "PhotoPicks",
    "slug": "photopicks",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/app-icons/app-icon-1024.png",
    "userInterfaceStyle": "light",
    "splash": {
  "image": "./assets/app-icons/app-icon-1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0e1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.photopicks",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "PhotoPicks needs access to your photo library to help you organize and manage your photos.",
        "NSPhotoLibraryAddUsageDescription": "PhotoPicks needs permission to save organized photos back to your library.",
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "Image",
            "CFBundleTypeRole": "Editor",
            "LSItemContentTypes": [
              "public.image",
              "public.png",
              "public.jpeg"
            ],
            "LSHandlerRank": "Alternate"
          }
        ],
        "UTExportedTypeDeclarations": [
          {
            "UTTypeIdentifier": "public.image",
            "UTTypeReferenceURL": "http://www.apple.com/",
            "UTTypeDescription": "Image",
            "UTTypeConformsTo": [
              "public.data",
              "public.content"
            ],
            "UTTypeTagSpecification": {
              "public.filename-extension": ["jpg", "jpeg", "png", "gif", "bmp", "tiff"],
              "public.mime-type": ["image/jpeg", "image/png", "image/gif", "image/bmp", "image/tiff"]
            }
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icons/app-icon-1024.png",
        "backgroundColor": "#0a0e1a"
      },
      "permissions": [
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.anonymous.photopicks"
    },
    "web": {
      "favicon": "./assets/app_icon_photo_picks.png"
    }
  }
}
